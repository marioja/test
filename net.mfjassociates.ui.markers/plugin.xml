<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="net.mfjassociates.ui.markers.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="initial"
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ui.category.file"
            defaultHandler="net.mfjassociates.ui.markers.handlers.MarkersHandler"
            id="net.mfjassociates.ui.markers.command1"
            name="%Markers.export.command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            class="net.mfjassociates.ui.markers.handlers.ExportContribution"
            locationURI="net.mfjassociates.ui.markers.menuContribution1">
      </menuContribution>
   </extension>

</plugin>
